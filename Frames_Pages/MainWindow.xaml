<Window x:Name="main_window" x:Class="Frames_Pages.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Frames_Pages"
        mc:Ignorable="d"
 
        Title="PasswordManager"  WindowStartupLocation="CenterScreen" ResizeMode="CanResize" MinHeight="500" MinWidth="500">
    <Window.Background>
        <ImageBrush ImageSource="Resources/bgDarkEnhanced.jpg"/>
    </Window.Background>
    <Viewbox Stretch="Uniform">
        <Frame x:Name="frame" Content=""  NavigationUIVisibility="Hidden" />
    </Viewbox>
    
    <Window.Icon>
        <BitmapImage UriSource="\Resources\padlockicon.png" ></BitmapImage>
    </Window.Icon>
</Window>

    